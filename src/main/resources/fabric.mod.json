{
  "schemaVersion": 1,
  "id": "vanillarpc",
  "version": "${version}",
  "name": "Vanilla RPC",
  "description": "Vanilla Discord RPC for modded Minecraft",
  "authors": [
    "NikSne"
  ],
  "contact": {
    "email": "contact@niksne.ru",
    "sources": "https://github.com/NikSneMC/VanillaRPC",
    "issues": "https://github.com/NikSneMC/VanillaRPC/issues",
    "homepage": "https://niksne.ru"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/vanillarpc/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "ru.niksne.vanillarpc.VanillaRPC"
    ]
  },
  "depends": {
    "fabricloader": "*",
    "minecraft": ">=1.14"
  },
  "breaks": {
    "simple-rpc": "*",
    "red_discord_presence": "*",
    "craftpresence":  "*",
    "simplystatus": "*",
    "sdrp": "*",
    "basicdiscordrichpresence": "*",
    "rederevodiscordrichpresence": "*",
    "bbmcord": "*",
    "yarpc": "*",
    "fabripresence": "*",
    "essential": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modrinth": "https://modrinth.com/mod/vanilla-rpc",
        "discord": "https://discord.com/invite/hGxQgrYTD3",
        "Boosty": "https://boosty.to/niksne",
        "Support me": "https://www.donationalerts.com/r/niksne"
      }
    }
  }
}
